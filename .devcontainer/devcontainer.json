{
	"name": "oci-spec-rs",
	"image": "mcr.microsoft.com/devcontainers/rust:1-bullseye",
	"customizations": {
		"vscode": {
			"settings": {
				"files.watcherExclude": {
					"**/target/**": true
				},
				"rust-analyzer.checkOnSave.command": "clippy"
			},
			"extensions": [
				"rust-lang.rust-analyzer",
				"tamasfe.even-better-toml"
			]
		}
	}
}